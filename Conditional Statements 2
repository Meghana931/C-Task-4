
#include <stdio.h>

int main() 
 {
printf("pick an item: \n0 .engineering \n1 .medical science");
int choice=0;
scanf("%d",&choice);
switch(choice)
{
    case 0:
    printf("you choose to be an engineer");
    break;
    case 1:
    printf("you choose to be a doctor");
    break;
    default: printf("Invalid choice");
}
return 0;
 }
